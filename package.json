{
  "name": "census-responses",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run copyEsriAssets && vue-cli-service serve",
    "build": "npm run copyEsriAssets && vue-cli-service build",
    "lint": "npm run copyEsriAssets && vue-cli-service lint",
    "test": "npm run copyEsriAssets && jest",
    "copyEsriAssets": "ncp ./node_modules/@arcgis/core/assets ./public/assets"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.17",
    "@arcgis/core": "^4.18.1",
    "core-js": "^3.6.5",
    "leaflet": "^1.7.1",
    "ncp": "^2.0.0",
    "node-fetch": "^2.6.1",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^26.6.3",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
